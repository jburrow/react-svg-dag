{
  "name": "react-svg-dag",
  "version": "0.0.16",
  "description": "React simple svg dag",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://jburrow.github.io/react-svg-dag/docs/",
  "prettier": {
    "printWidth": 120
  },
  "scripts": {
    "updateAll": "ncu -u",
    "start": "webpack serve",
    "compile": "tsc && webpack --mode=production && typedoc --out ./docs ./src/index.ts",
    "tsc": "tsc --watch",
    "doc": "typedoc --out ./docs ./src/index.ts",
    "test": "jest --coverage",
    "unit-test": "jest --coverage",
    "integration-test": "cypress run --component",
    "test-todo": "concurrently \"npm run unit-test\" \"npm run integration-test\""
  },
  "author": "James Burrow",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.* || ^17.* || ^18.*",
    "react-dom": "^16.* || ^17.* || ^18.*"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "react-error-boundary": "^3.1.4",
    "svg-pan-zoom": "^3.6.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/runtime": "7.20.13",
    "@babel/types": "7.20.7",
    "@types/debug": "^4.1.7",
    "@types/jest": "29.4.0",
    "@types/react": "18.0.28",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "@webpack-cli/serve": "1.7.0",
    "babel-jest": "29.4.2",
    "concurrently": "7.6.0",
    "cypress": "12.5.1",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.4.2",
    "jest-environment-jsdom": "29.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typedoc": "0.23.24",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  }
}
