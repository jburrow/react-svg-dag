{
  "name": "react-svg-dag",
  "version": "0.0.18",
  "description": "React simple svg dag",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://jburrow.github.io/react-svg-dag/docs/",
  "prettier": {
    "printWidth": 120
  },
  "scripts": {
    "updateAll": "ncu -u",
    "start": "webpack serve",
    "compile": "tsc && webpack --mode=production && typedoc --out ./docs ./src/index.ts",
    "tsc": "tsc --watch",
    "doc": "typedoc --out ./docs ./src/index.ts",
    "test": "jest --coverage",
    "unit-test": "jest --coverage",
    "integration-test": "cypress run --component",
    "test-todo": "concurrently \"npm run unit-test\" \"npm run integration-test\"",
    "postinstall": "patch-package"
  },
  "author": "James Burrow",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.* || ^17.* || ^18.*",
    "react-dom": "^16.* || ^17.* || ^18.*"
  },
  "dependencies": {
    "@react-hook/resize-observer": "^1.2.6",
    "dagre": "^0.8.5",
    "debug": "^4.3.4",
    "patch-package": "^6.5.1",
    "react-error-boundary": "^3.1.4",
    "resize-observer-polyfill": "^1.5.1",
    "svg-pan-zoom": "^3.6.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/runtime": "7.28.4",
    "@babel/types": "7.28.4",
    "@types/dagre": "^0.7.47",
    "@types/debug": "^4.1.7",
    "@types/jest": "29.5.14",
    "@types/react": "18.3.24",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "@webpack-cli/serve": "1.7.0",
    "babel-jest": "29.7.0",
    "concurrently": "9.2.1",
    "cypress": "14.5.4",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-test-renderer": "18.3.1",
    "ts-jest": "29.4.1",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typedoc": "^0.28.0",
    "typescript": "^5.0.2",
    "webpack": "5.101.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "5.2.2"
  }
}